{
  "framework": "node",
  "buildCommand": "yarn nx build devspace-api",
  "outputDirectory": "apps/devspace-api/dist",
  "installCommand": "yarn install --frozen-lockfile",
  "builds": [
    {
      "src": "dist/apps/devspace-api/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/apps/devspace-api/main.js"
    }
  ]
}
